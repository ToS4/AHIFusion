<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:utum="using:Uno.Toolkit.UI.Material">

  <ResourceDictionary.MergedDictionaries>
    <!-- Load WinUI resources -->
    <XamlControlsResources xmlns="using:Microsoft.UI.Xaml.Controls" />
    <utum:MaterialToolkitTheme
      ColorOverrideSource="ms-appx:///AHIFusion/Styles/ColorPaletteOverride.xaml">
      <!-- NOTE: You can override the default Roboto font by providing your font assets here. -->
      <utum:MaterialToolkitTheme.FontOverrideDictionary>
        <ResourceDictionary>
          <FontFamily x:Key="MaterialLightFontFamily">ms-appx:///Assets/Fonts/aptos-light.ttf#Aptos</FontFamily>
          <FontFamily x:Key="MaterialMediumFontFamily">ms-appx:///Assets/Fonts/aptos.ttf#Aptos</FontFamily>
          <FontFamily x:Key="MaterialRegularFontFamily">ms-appx:///Assets/Fonts/aptos-semibold.ttf#Aptos</FontFamily>
        </ResourceDictionary>
      </utum:MaterialToolkitTheme.FontOverrideDictionary>
    </utum:MaterialToolkitTheme>
    <ResourceDictionary Source="CustomStyles.xaml"></ResourceDictionary>
    <ResourceDictionary Source="ms-appx:///AHIFusion/Styles/ColorPaletteOverride.xaml"/>
  </ResourceDictionary.MergedDictionaries>

  
</ResourceDictionary>
